<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Modernize Free</title>
  <link rel="shortcut icon" type="image/png" href="/src/assets/images/logos/favicon.png" />
  <link rel="stylesheet" href="/src/assets/css/styles.min.css" />
  <style>
    /* Additional CSS */
    .page-wrapper {
      display: flex;
      flex-direction: column;
      min-height: 100vh;
    }
    .body-wrapper {
      flex: 1;
    }
    .footer {
      flex-shrink: 0;
    }
    .main-content {
      padding: 20px;
    }
    .card {
      background: #fff;
      border: 1px solid #e3e6f0;
      border-radius: .35rem;
      padding: 15px;
      margin-bottom: 20px;
      box-shadow: 0 1px 3px rgba(0, 0, 0, .08);
    }
    .card-header {
      font-size: 1.25rem;
      font-weight: bold;
      margin-bottom: 10px;
    }
    .card-body {
      font-size: 1rem;
    }
    .chart-container {
      position: relative;
      margin: 20px 0;
    }
    .table-responsive {
      margin: 20px 0;
    }
    .table thead th {
      font-weight: bold;
      background-color: #f8f9fc;
    }
    .text-center {
      text-align: center;
    }
    .mt-4 {
      margin-top: 1.5rem;
    }
    .mb-4 {
      margin-bottom: 1.5rem;
    }
    .btn-add {
      margin-bottom: 1rem;
    }
  </style>
</head>

<body>
  <div class="container mt-3">
    <form action="/save/store" method="POST">
     
      <div class="mb-3">
        <label for="file" class="form-label">Nama file</label>
        <select class="form-select" name="id_file" id="id_file" required>
          <% for(var i = 0; i < file.length; i++){ %>
          <option value="<%= file[i].id_file %>">
            <%= file[i].file_pdf %>
          </option>
          <% } %>
        </select>
      </div>
      <input type="hidden" name="" />
      <!-- <div class="mb-3">
        <label for="pengguna" class="form-label">Pengguna</label>
        <select class="form-select" name="id_user" required>
          <% for(var i = 0; i < users.length; i++){ %>
          <option value="<%= user[i].id_user %>">
            <%= users[i].nama_pengguna %>
          </option>
          <% } %>
        </select>
      </div> -->

      <div class="mb-3">
        <button type="submit" class="btn btn-sm btn-success">Simpan</button>
      </div>
    </form>
  </div>
</div>
</div>
</div>
<script src="/src/assets/libs/jquery/dist/jquery.min.js"></script>
<script src="/src/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="/src/assets/js/sidebarmenu.js"></script>
<script src="/src/assets/js/app.min.js"></script>
<script src="/src/assets/libs/apexcharts/dist/apexcharts.min.js"></script>
<script src="/src/assets/libs/simplebar/dist/simplebar.js"></script>
<script src="/src/assets/js/dashboard.js"></script>
</body>
</html>
