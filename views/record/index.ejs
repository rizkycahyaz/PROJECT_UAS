<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Record Unduhan</title>
    <link rel="stylesheet" href="/path/to/your/css/styles.css" />
  </head>
  <body>
    <h1>Riwayat Unduhan</h1>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Nama File</th>
          <th>Email Pengguna</th>
          <th>Tanggal</th>
          <th>Aksi</th>
        </tr>
      </thead>
      <tbody>
        <% records.forEach(record => { %>
        <tr>
          <td><%= record.id_file %></td>
          <td><%= record.nama_file %></td>
          <td><%= record.email %></td>
          <td><%= record.tanggal %>/<%= record.bulan %>/<%= record.tahun %></td>
          <td>
            <a href="/record/delete/<%= record.id %>">Hapus</a>
          </td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    <a href="/file">Kembali ke Daftar File</a>
  </body>
</html>
